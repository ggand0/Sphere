<%= form_for(@model_datum) do |f| %>
  <% if @model_datum.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@model_datum.errors.count, "error") %> prohibited this model_datum from being saved:</h2>

      <ul>
      <% @model_datum.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :title%><br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <p>file</p>
    <%= f.file_field :file %>
  </div>

  <div class="files">
	<%= f.fields_for :texture, html: { class: "upload", multipart: true } do |t| %>
		<%= t.file_field 'model_datum[textures]', multiple: true %>
	<% end %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>