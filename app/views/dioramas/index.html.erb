<h1>Listing dioramas</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @dioramas.each do |diorama| %>
      <tr>
        <td><%= diorama.title %></td>
        <td><%= link_to 'Show', diorama %></td>
        <td><%= link_to 'Edit', edit_diorama_path(diorama) %></td>
        <td><%= link_to 'Destroy', diorama, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

